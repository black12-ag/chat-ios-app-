# Gemfile for MunrChat iOS Project - Updated 2025
# Ruby dependencies for iOS development tools
# Compatible with Ruby 2.6+ and modern iOS development

source 'https://rubygems.org'

# Core iOS Development Tools
gem 'cocoapods'
gem 'fastlane'
gem 'xcode-install'

# Database and Data Processing
gem 'anbt-sql-formatter'

# Development and Testing (optional)
group :development, :optional => true do
  gem 'pry'
end
